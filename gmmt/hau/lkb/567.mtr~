pro-insert-arg1-omtr := monotonic_omtr &
[ INPUT.RELS <! !>,
  CONTEXT.RELS <! [ ARG1 ref-ind & #x ] !>,
  FILTER.RELS <! [ ARG0 #x ] !>,
  OUTPUT [ RELS <! noun-relation &
		[ PRED "_pronoun_n_rel",
		ARG0 #x,
		LBL #larg ],
		quant-relation &
		[ PRED "_exists_q_rel",
		  ARG0 #x,
		  RSTR #harg ] !>,
	HCONS <! qeq &
		[ HARG #harg,
		  LARG #larg ] !> ],
  FLAGS.EQUAL < #x > ].