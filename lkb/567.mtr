dem-gen-omtr := monotonic_omtr &
 [ INPUT.RELS <! [ PRED distal+dem_a_rel,
                   LBL #h,
                   ARG1 #x ] !>,
   OUTPUT.RELS <! [ PRED distal+dem_a_rel,
                    LBL #h,
                    ARG1 #x ],
                  [ PRED dem-rule-pred,
                    ARG0 #x ] !>,
   FILTER.RELS <! [ PRED dem-rule-pred,
                    ARG0 #x ] !> ].

pro-insert-arg1-omtr := monotonic_omtr &
[ INPUT [ HOOK.INDEX #e,
	  RELS <! !> ],
  CONTEXT.RELS <! [ ARG1 ref-ind & #x &
			 [ COG-ST #cs,
			   PNG #png ]] !>,
  FILTER.RELS <! [ ARG0 #x ] !>,
  OUTPUT [ HOOK.INDEX #e,
	   RELS <! noun-relation &
		[ PRED "_pronoun_n_rel",
		ARG0 #x &
		       [ COG-ST #cs,
			 PNG #png ],
		LBL #larg ],
		quant-relation &
		[ PRED "_exist_q_rel",
		  ARG0 #x,
		  RSTR #harg ] !>,
	HCONS <! qeq &
		[ HARG #harg,
		  LARG #larg ] !> ],
  FLAGS.EQUAL < #x > ].

